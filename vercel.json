{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node@3.0.0"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build@2.0.0"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/([a-zA-Z0-9]{6})",
      "dest": "/api/[shortCode]?shortCode=$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
